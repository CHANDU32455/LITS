{
  "expo": {
    "name": "Safety First - Injury Management",
    "slug": "safetyfirst",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "litsbizil",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "supabaseUrl": "$EXPO_PUBLIC_SUPABASE_URL",
      "supabaseAnonKey": "$EXPO_PUBLIC_SUPABASE_ANON_KEY",
      "resetPasswordRedirect": "$EXPO_PUBLIC_RESET_PASSWORD_REDIRECT",
      "authCallbackRedirect": "$EXPO_PUBLIC_AUTH_CALLBACK_REDIRECT",
      "environment": "$EXPO_PUBLIC_ENVIRONMENT",
      "router": {}
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.litsbizil.app",
      "associatedDomains": [
        "applinks:lits-bizil.netlify.app"
      ]
    },
    "android": {
      "package": "com.litsbizil.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "lits-bizil.netlify.app"
            },
            {
              "scheme": "litsbizil"
            },
            {
              "scheme": "http",
              "host": "localhost",
              "port": "8081"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
